      <tag name="Reference.ChangeIndicator" value="N"/>
    </object>
    <object>
      <id>PHR_R2456</id>
      <name>PH.5.5#02</name>
      <notes>The system SHOULD provide the ability to capture, maintain, and render configuration parameters regarding alerts, reminders, and/or notifications.</notes>
      <stereotype>Criteria</stereotype>
      <type>Requirement</type>
      <parentId>PHR_R2454</parentId>
